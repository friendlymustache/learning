
<div class="ui centered header"> {{name}} </div>

<div class="ui link list">
	{{#each link in links}}
			<a target="_blank" {{bind-attr href="link.url"}}>{{link.title}}</a>
	{{/each}}

	{{#each topic in children}}
		{{#link-to 'topic' topic}} {{topic.name}} {{/link-to}}
	{{/each}}
</div>

{{outlet}}